{
  "name": "amfe-flexible",
  "version": "2.2.1",
  "description": "Build flexible page on mobile platform",
  "keywords": [
    "lib",
    "amfe",
    "flexible",
    "mobile",
    "css",
    "rem",
    "vw",
    "esm"
  ],
  "homepage": "https://github.com/amfe/lib-flexible",
  "repository": {
    "type": "git",
    "url": "git@github.com:amfe/lib-flexible.git"
  },
  "author": [{
    "name": "mingelz",
    "email": "mingelz@gmail.com"
  }],
  "main": "dist/iife/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/iife/index.js"
    }
  },
  "scripts": {
    "compress": "uglifyjs index.js -o index.min.js -c -m",
    "build": "tsup src/index.ts --format iife,esm --minify --dts --sourcemap --out-dir dist --legacy-output"
  },
  "devDependencies": {
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "uglify-js": "^3.0.27"
  },
  "license": "MIT"
}
